    "import random\n",
    "\n",
    "print(\"input your fibonacci limit:\")\n",
    "limit = input(\"> \")\n",
    "\n",
    "int_limit = int(limit)\n",
    "i=0\n",
    "a=[1,1]\n",
    "\n",
    "while i < int_limit:\n",
    "    b = i\n",
    "    b = a[i+1]+int(a[i])\n",
    "    c = b - int_limit\n",
    "    if c<=0:\n",
    "        a.append(b)\n",
    "        i=i+1\n",
    "    else:\n",
    "        break\n",
    "print(a)\n"
